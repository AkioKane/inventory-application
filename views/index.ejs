      <div class="container-main">
        <header class="category-header">
          <a class="category-link" href="?category=pistol">Pistol</a>
          <a class="category-link" href="?category=rifle">Rifle</a>
          <a class="category-link" href="?category=sniperRifle">Sniper Rifle</a>
          <a class="category-link" href="?category=smg">SMG</a>
          <a class="category-link" href="?category=machinegun">Machinegun</a>
          <a class="category-link" href="?category=shotgun">Shotgun</a>
          <a class="category-link" href="?category=knife">Knife</a>
          <a class="category-link" href="?category=gloves">Gloves</a>
          <a class="category-link" href="?category=container">Container</a>
          <a class="category-link" href="?category=stickers">Stickers</a>
          <a class="category-link" href="?category=other">Other</a>
        </header>
        <!-- <div class="search-panel">
          <input type="text" class="search-input" placeholder="Quick Search">
        </div> -->
        <div class="items">
          <% for (item of items)  { %>
            <div class="item <%=item.rarity.charAt(0).toLowerCase() + item.rarity.slice(1)%>">
              <div class="buy-item-container">
                <button class="buy-item">Buy</button>
              </div>
              <div class="img-container-item">
                <img src="<%=item.imgurl %>" alt="image">
              </div>
              <div class="info-container-item">
                <h4>
                  <span>&#8381;<%=item.cost %></span>
